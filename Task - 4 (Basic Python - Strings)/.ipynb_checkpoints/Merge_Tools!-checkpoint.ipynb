    "def merge_the_tools(string, k):\n",
    "    l = []\n",
    "    m = 0\n",
    "    for i in range(len(string)):\n",
    "        l.append(string[m:m+k])\n",
    "        m = m+k\n",
    "    for i in l:\n",
    "        print(''.join(list(dict.fromkeys(list(i)).keys())))"
   ]
